# 医保接口SDK环境变量配置示例
# 请复制此文件为 .env 并根据实际情况修改配置

# 基础配置
# 运行环境 (development/testing/production)
MEDICAL_INSURANCE_ENV=development
# 是否启用调试模式
MEDICAL_DEBUG=true

# 数据库配置
# 数据库主机地址
MEDICAL_DB_HOST=localhost
# 数据库端口
MEDICAL_DB_PORT=3306
# 数据库名称
MEDICAL_DB_NAME=medical_insurance_dev
# 数据库用户名
MEDICAL_DB_USER=root
# 数据库密码
MEDICAL_DB_PASSWORD=
# 数据库连接池大小
MEDICAL_DB_POOL_SIZE=10

# Redis配置
# Redis主机地址
MEDICAL_REDIS_HOST=localhost
# Redis端口
MEDICAL_REDIS_PORT=6379
# Redis数据库编号
MEDICAL_REDIS_DB=0
# Redis密码
# MEDICAL_REDIS_PASSWORD=

# 日志配置
# 日志级别
MEDICAL_LOG_LEVEL=DEBUG
# 日志文件路径
MEDICAL_LOG_FILE=logs/medical_insurance_sdk.log

# 安全配置
# 加密密钥
# MEDICAL_ENCRYPTION_KEY=2wjpKEdGPAjZGzFXMOxft1XMe0v5Oh2s8b0QIMYHUj8=
# 默认加密类型
MEDICAL_CRYPTO_TYPE=SM4
# 默认签名类型
MEDICAL_SIGN_TYPE=SM3

# HTTP客户端配置
# HTTP请求超时时间（秒）
MEDICAL_HTTP_TIMEOUT=30
# HTTP最大重试次数
MEDICAL_HTTP_MAX_RETRIES=3

# 异步处理配置
# Celery代理URL
MEDICAL_CELERY_BROKER=redis://localhost:6379/1
# Celery结果后端URL
MEDICAL_CELERY_BACKEND=redis://localhost:6379/2

# 配置文件相关
# 配置文件目录
# MEDICAL_CONFIG_DIR=./config/environments
# 指定配置文件路径
# MEDICAL_CONFIG_FILE=./config/custom.yaml